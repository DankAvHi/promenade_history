generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x", "windows"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  iduser Int    @id @unique(map: "iduser_UNIQUE")
  vkid   String @unique(map: "vkid_UNIQUE") @db.VarChar(120)
}
