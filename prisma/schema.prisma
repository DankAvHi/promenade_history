generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x", "windows"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  iduser        Int     @id @unique(map: "iduser_UNIQUE")
  vkid          Int     @unique(map: "vkid_UNIQUE")
  ticket_active Boolean @default(false)
  tokens        String? @db.LongText
  name          String  @db.Text
}
