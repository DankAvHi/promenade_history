generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x", "windows"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  iduser        Int     @id @unique(map: "iduser_UNIQUE") @default(autoincrement())
  vkid          Int     @unique(map: "vkid_UNIQUE")
  email         String  @unique(map: "email_UNIQUE") @db.VarChar(320)
  ticket_active Boolean @default(false)
  first_name    String  @db.VarChar(120)
  last_name     String  @db.VarChar(120)
}
